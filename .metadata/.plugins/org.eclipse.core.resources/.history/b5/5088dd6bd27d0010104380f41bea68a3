package modelo;

public class Numero {
private int n;

public int sumar(int n1){
	return n1+this.n;
	
}
public int multiplicar(int n1) {
	return n1*n;
	
}

public boolean esPrimo(int n) {
    boolean flag = true;
    	for(int i=2;i<n;i++) {
    		if(n%i==0) {
    			flag=false;
    			return flag;
    		}
    		else {
    			flag=false;
    		}
    	}
    
    
    
    
    return flag;
}

public Numero(int n) {
	super();
	this.n = n;
}
public int getN() {
	return n;
}
public void setN(int n) {
	this.n = n;
}








}
