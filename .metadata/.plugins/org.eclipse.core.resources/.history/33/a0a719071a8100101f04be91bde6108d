package test;
import modelo.ArregloBidimencional;
public class Test {

	public static void main(String[] args) {
		
		double[][] matriz= {{3,1},{3,3}};

		double[][] matriz2= {{3},{3}};
		ArregloBidimencional b=new ArregloBidimencional(matriz2);
		double [][]c=b.sumar(matriz);
		b.mostrar(c);
		double [][]d=b.restar(matriz);

		b.mostrar(d);

		double [][]e=b.traspuesta();
		b.mostrar(e);
		double [][]r=b.multiplicar(matriz2);
		b.mostrar(r);
		
		double [][]p=b.multiplicar(10.3);
		b.mostrar(p);
		
		
		
	}
	

}
