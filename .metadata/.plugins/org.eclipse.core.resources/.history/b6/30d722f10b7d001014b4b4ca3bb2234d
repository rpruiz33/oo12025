package modelo;

public class Numero {
private int n;

public int sumar(int n1){
	return n1+this.n;
	
}
public int multiplicar(int n1) {
	return n1*n;
	
}

public boolean esPrimo() {
	
    if (n <=1) {
        return false; 
    }
    for (int i = 1; i < Math.sqrt(n); i++) {
        if (n % i == 0) {
            return false;
        }
    }
    return true;
}
public Numero(int n) {
	super();
	this.n = n;
}
public int getN() {
	return n;
}
public void setN(int n) {
	this.n = n;
}








}
