package test;
import modelo.Numero;
public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Numero n=new Numero(3);
		
		System.out.println(n.sumar(2));
		System.out.println(n.multiplicar(3));
		System.out.println(n.esPar());
		System.out.println(n.esPrimo(1));
		System.out.println(n.convertirAString());
		System.out.println(n.convertirDouble());
		System.out.println(n.calcularPotencia(2));
		System.out.println(n.calcularFactorial());
	}

}
