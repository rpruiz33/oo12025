package test;
import modelo.ArregloUnidimensional; 
public class Test {

	public static void main(String[] args) {
		int[] vector = {1,2,3,4,5,6,7,8,9};
		
		ArregloUnidimensional a= new ArregloUnidimensional( vector);
		a.mostrar();
		System.out.println(a.traerElMenor());
		System.out.println(a.traerElMayor());
		System.out.println(a.calcularElmpromedio());
		a.ordenarAscendente();
		a.mostrar();
		a.ordenarDesendente();
		a.mostrar();
		System.out.println(a.traerFrecuencia(5));
		
		System.out.println(a.traerModa());
		
	}

}
